# top-most EditorConfig file
root = true

# Unix-style newlines with a newline ending every file
[*]
end_of_line = lf
insert_final_newline = true
charset = utf-8


# Tab indentation (no size specified)
[Makefile]
indent_style = tab

[ui/*]
indent_style = space

[ui/**.js]
indent_size = 2

[ui/**.scss]
indent_size = 4